  "scripts": {
    "dev": "yarn && ./bin/webpack-dev-server",
    "lint": "npx eslint --cache --ext .js,.ts,.tsx app/javascript",
    "types": "yarn types:backend && yarn types:frontend",
    "types:backend": "bundle exec rake graphql:schema:idl",
    "types:frontend": "npx concurrently --kill-others-on-fail  \"yarn types:frontend:types\" \"yarn types:frontend:fragments\"",
    "types:frontend:types": "rm -rf app/javascript/schema && apollo client:codegen app/javascript/schema -c=apollo.config.js --target=typescript --outputFlat",
    "types:frontend:fragments": "graphql-codegen --config ./codegen.yml"
  }
